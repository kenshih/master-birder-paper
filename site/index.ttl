obo:NCBITaxon_8839
  rdfs:label "Mallard"@en ;
  mb:hasAvibaseIdentifier mb:AvibaseID_85625D75 .

mb:AvibaseID_85625D75
  a mb:AvibaseIdentifier ;
  mb:identifierValue "85625D75";
  rdfs:seeAlso <https://avibase.bsc-eoc.org/species.jsp?avibaseid=avibase-85625D75> .
